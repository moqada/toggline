{
  "name": "toggline",
  "description": "Toggl Timeline Viewer",
  "version": "0.0.0",
  "author": "moqada <moqada@gmail.com>",
  "bugs": {
    "url": "https://github.com/moqada/toggline/issues"
  },
  "dependencies": {
    "base64-js": "^1.2.0",
    "dacho": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.17.5",
    "browserify": "^14.3.0",
    "browserify-hmr": "^0.3.5",
    "chokidar-cli": "^1.2.0",
    "codecov": "^2.2.0",
    "cpx": "^1.5.0",
    "envify": "^4.0.0",
    "eslint": "^3.8.1",
    "eslint-config-moqada": "^18.0.0",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-jsdoc": "^3.1.0",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.46.0",
    "gh-pages": "^1.0.0",
    "isparta": "^4.0.0",
    "jade": "^1.11.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.1",
    "react-proxy": "^1.1.8",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.0",
    "stylus": "^0.54.5",
    "watchify": "^3.7.0"
  },
  "homepage": "https://github.com/moqada/toggline#readme",
  "keywords": [],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/moqada/toggline.git"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:copy": "cpx 'src/**/*.png' dist/",
    "build:css": "stylus src/styles/app.styl -o dist/styles/app.css",
    "build:js": "NODE_ENV=production browserify -t babelify -t envify src/app.js -o dist/app.js",
    "build:jade": "jade src --out dist",
    "codecov": "codecov",
    "deploy": "npm-run-all build gh-pages",
    "gh-pages": "gh-pages -d dist",
    "prebuild": "rm -rf dist && mkdir -p dist/styles",
    "pretest": "rm -rf coverage",
    "prestart": "mkdir -p dist/styles",
    "server": "browser-sync start --files 'dist/styles/*.css' --server dist --no-open",
    "start": "npm-run-all --parallel server watch",
    "stop": "flow stop",
    "test": "npm-run-all test:*",
    "test:flow": "flow",
    "test:lint": "eslint --cache {src,test}",
    "watch": "npm-run-all --parallel watch:*",
    "watch:copy": "cpx --watch 'src/**/*.png' dist/",
    "watch:css": "stylus -w src/styles/app.styl -o dist/styles/app.css",
    "watch:flow": "flow & chokidar src/ -c flow",
    "watch:jade": "jade src --watch --out dist",
    "watch:lint": "chokidar src test -c 'npm run -s test:lint'",
    "watch:js": "watchify -v -t babelify -p browserify-hmr src/app.js -o dist/app.js"
  }
}
