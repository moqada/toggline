{
  "name": "toggline",
  "description": "Toggl Timeline Viewer",
  "version": "0.0.0",
  "author": "moqada <moqada@gmail.com>",
  "bugs": {
    "url": "https://github.com/moqada/toggline/issues"
  },
  "dependencies": {
    "base64-js": "^1.0.2",
    "dacho": "^1.0.0",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-plugin-espower": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chokidar-cli": "^1.2.0",
    "codecov.io": "^0.1.6",
    "cpx": "^1.2.1",
    "envify": "^3.4.0",
    "eslint": "^1.10.3",
    "eslint-config-moqada": "^9.0.0",
    "eslint-plugin-react": "^3.13.1",
    "flow-bin": "^0.20.1",
    "gh-pages": "^0.8.0",
    "isparta": "^4.0.0",
    "jade": "^1.11.0",
    "mocha": "^2.3.4",
    "npm-run-all": "^1.4.0",
    "power-assert": "^1.2.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "stylus": "^0.53.0",
    "watchify": "^3.6.1"
  },
  "homepage": "https://github.com/moqada/toggline#readme",
  "keywords": [],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/moqada/toggline.git"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:copy": "cpx 'src/**/*.png' dist/",
    "build:css": "stylus src/styles/app.styl -o dist/styles/app.css",
    "build:js": "NODE_ENV=production browserify -t babelify -t envify src/app.js -o dist/app.js",
    "build:jade": "jade src --out dist",
    "codecov": "cat coverage/lcov.info | codecov",
    "deploy": "npm-run-all build gh-pages",
    "gh-pages": "gh-pages -d dist",
    "lint": "eslint --cache {src,test}",
    "prebuild": "rm -rf dist && mkdir -p dist/styles",
    "pretest": "rm -rf coverage",
    "prestart": "mkdir -p dist/styles",
    "start": "npm-run-all watch",
    "stop": "flow stop",
    "test": "npm-run-all lint test:*",
    "test:flow": "flow",
    "watch": "npm-run-all --parallel watch:*",
    "watch:copy": "cpx --watch 'src/**/*.png' dist/",
    "watch:css": "stylus -w src/styles/app.styl -o dist/styles/app.css",
    "watch:flow": "flow & chokidar src/ -c flow",
    "watch:jade": "jade src --watch --out dist",
    "watch:lint": "chokidar src test -c 'npm run -s lint'",
    "watch:js": "watchify -v -t babelify src/app.js -o dist/app.js"
  }
}
